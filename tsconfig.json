{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
		"paths": {
			"@auth": ["src/auth/index"],
			"@components/*": ["src/components/*"],
			"@data/*": ["src/data/*"],
			"@db/*": ["src/db/*"],
			"@models/*": ["src/models/*"],
			"@pages/*": ["src/pages/*"],
			"@stores/*": ["src/stores/*"],
		},
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
